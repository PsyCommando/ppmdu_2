<?xml version="1.0" encoding="UTF-8"?>
<AnimationListTemplate>
    <!--This contains the names for all the required character animations for regular pokemons.-->
    <!--Those 13 animation groups are required!-->

    <!--Group 0: Run Animations -->
    <!--The run cycle when moving around in dungeons or anywhere else.-->
    <AnimGroup>
        <!-- Name for the current group -->
        <Name>Run</Name>
        <!-- Contains 8 sequence(s)) -->
        <AnimSequences>
            <AnimSequence>
                <Name>Down</Name>
            </AnimSequence>
            <AnimSequence>
                <Name>Down-Left</Name>
            </AnimSequence>
            <AnimSequence>
                <Name>Left</Name>
            </AnimSequence>
            <AnimSequence>
                <Name>Up-Left</Name>
            </AnimSequence>
            <AnimSequence>
                <Name>Up</Name>
            </AnimSequence>
            <AnimSequence>
                <Name>Up-Right</Name>
            </AnimSequence>
            <AnimSequence>
                <Name>Right</Name>
            </AnimSequence>
            <AnimSequence>
                <Name>Down-Right</Name>
            </AnimSequence>
        </AnimSequences>
    </AnimGroup>
    
    <!--Group 1: Running Slow Animations -->
    <AnimGroup>
        <!-- Name for the current group -->
        <Name>Running Slow</Name>
        <!-- Contains 0 sequence(s)) -->
